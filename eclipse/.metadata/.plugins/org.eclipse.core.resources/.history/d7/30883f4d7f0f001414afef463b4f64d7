package com.emicb.thehuntermod.item;

import com.emicb.thehuntermod.creativetab.CreativeTabTHM;

import net.minecraft.item.Item;
import net.minecraft.item.ItemArmor;
import net.minecraft.item.ItemArmor.ArmorMaterial;

public class ItemCamoArmor extends ItemArmor
{
	public ItemCamoArmor(ArmorMaterial material, int id, int placement)
	{
		super(material, id, placement);
		this.setCreativeTab(CreativeTabTHM.THM_TAB);
		this.setUnlocalizedName("camoArmor");
	}
}
